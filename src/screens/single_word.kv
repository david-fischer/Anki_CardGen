#:import SingleWordMain screens.single_word_new.SingleWordMain
SingleWordMain:
    id: word_prop
    scroll_view: word_scroll
    ScrollView:
        id: word_scroll
        do_scroll_x: False
        do_scroll_y: True
        size_hint: 1,1

    MDFloatingActionButton:
        icon: "check-bold"
        pos: (root.width-dp(16)-self.width,dp(10))
        on_press: word_prop.complete_generation()
