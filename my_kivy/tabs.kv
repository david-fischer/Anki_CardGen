#:import WordProperties my_kivy.user_input.WordProperties
#:import ImagePickerMain my_kivy.picture_picker.ImagePickerMain
#:import Window kivy.core.window.Window
<MyTabs>:
    on_parent: root.ids.carousel.ignore_perpendicular_swipes = True;root.ids.carousel.min_move = 0.1;
    EditTab:
        text:"file-document-edit-outline"

    ImageTab:
        id: image_picker_main
        text: "image-search-outline"

    PreviewTab:
        text: "eye"



<EditTab@BoxLayout+MDTabsBase>:
    ScrollView:
        do_scroll_x: False
        do_scroll_y: True
        size_hint: 1,1
        WordProperties:
            size_hint:1,None
            height: self.minimum_height

<ImageTab@Tab>:
    ImagePickerMain

<PreviewTab@Tab>:
