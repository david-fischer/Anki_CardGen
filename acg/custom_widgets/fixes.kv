<MDTabsCarousel>:
    min_move: 0.05
    ignore_perpendicular_swipes: True

<MyFloatingActionButtonSpeedDial@MDFloatingActionButtonSpeedDial+SpecificBackgroundColorBehavior>:
    label_text_color: self.specific_text_color
    background_palette: "Accent"

<MDFloatingRootButton,MDFloatingBottomButton>:
    -md_bg_color: app.theme_cls.accent_color
    -background_palette: "Accent"

<MDFloatingLabel>:
    -bg_color: app.theme_cls.accent_color
    -text_color: []

